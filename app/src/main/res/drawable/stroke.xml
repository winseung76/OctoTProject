<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="10dp"/>
            <stroke
                android:width="1.5dp"
                android:color="#D8D8D8" />
            <solid android:color="#FFF" />
        </shape>
    </item>

</layer-list>

